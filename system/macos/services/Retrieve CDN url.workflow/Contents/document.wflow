					<key>COMMAND_STRING</key>
					<string>cdn_endpoint="https://cdn.adlimen.com/"
rclone_remote="remote-cdn"
sync_folder="$HOME/media/cdn" # Definisci la tua cartella di sincronizzazione locale

# Ottieni il percorso completo del file selezionato usando AppleScript
filePath=$(osascript -e '
    tell application "Finder"
        set theSelection to selection as alias list
        if (count of theSelection) &gt; 0 then
            set theFile to item 1 of theSelection
            set theFilePath to POSIX path of theFile
            return theFilePath
        else
            return ""
        end if
    end tell
')

if [ -n "$filePath" ]; then
    # Calcola il percorso relativo rispetto alla cartella di sincronizzazione
    relative_path=$(echo "$filePath" | sed "s|^${sync_folder}/||")

    rclone_output="$(/opt/homebrew/bin/rclone ls "$rclone_remote:media-adlimen/$relative_path" 2&gt;&amp;1)"

    # DEBUG - Mostra l'output di rclone ls (commenta per uso normale)
    # osascript -e "display notification \"Output di rclone ls: '$rclone_output'\" with title \"DEBUG\""

    # Estrai il nome del file (dovrebbe ora includere il percorso relativo)
    rclone_found_path=$(echo "$rclone_output" | awk '{print $2}')

    if [ -n "$rclone_found_path" ]; then
        # Costruisci l'URL del CDN usando il percorso relativo e codifica ALTRI caratteri speciali
        encoded_path=$(echo "$relative_path" | sed 's/ /%20/g; s/â€“/%E2%80%93/g; s/#/%23/g; s/&amp;/%26/g; s/?/%3F/g; s/=/%3D/g; s/!/%21/g; s/'\''/%27/g; s/(/%28/g; s/)/%29/g; s/*/%2A/g; s/+/%2B/g; s/,/%2C/g; s/:/%3A/g; s/;/%3B/g; s/\\/%5C/g')
        cdn_url="$cdn_endpoint$encoded_path"
        echo "$cdn_url" | pbcopy
        osascript -e "display notification \"CDN URL copied to clipboard:\\n'$cdn_url'\" with title \"Success\""
    else
        osascript -e 'display notification "File not found on R2 or rclone error (dopo ls)" with title "Error"'
    fi
else
    osascript -e 'display notification "No file selected in Finder" with title "Error"'
fi</string>
